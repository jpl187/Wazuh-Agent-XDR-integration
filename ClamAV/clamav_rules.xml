<group name="clamav,malware,">
  <rule id="100100" level="12">
    <if_sid>5712</if_sid>
    <match>FOUND$</match>
    <description>ClamAV: Malware detected</description>
    <group>malware,</group>
  </rule>

  <rule id="100101" level="12">
    <if_sid>100100</if_sid>
    <regex>(\S+): (\S+) FOUND</regex>
    <description>ClamAV: $(matched2) detected in $(matched1)</description>
  </rule>
</group>
