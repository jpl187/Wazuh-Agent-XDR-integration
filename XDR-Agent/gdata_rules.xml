<group name="gdata,">
  <!-- Rule for G Data detection events -->
  <rule id="100100" level="10">
    <if_sid>571</if_sid>
    <match>detect</match>
    <description>G Data Agent Detection Alert</description>
    <group>mxdr,gdata,</group>
  </rule>
  
  <!-- Optional: Suppress all other G Data log entries -->
  <rule id="100101" level="0">
    <if_sid>571</if_sid>
    <description>G Data Agent - Normal logs (suppressed)</description>
    <group>mxdr,gdata,</group>
  </rule>
</group>
